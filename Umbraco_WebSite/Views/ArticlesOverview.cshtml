@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.ArticlesOverview>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
	Layout = "master.cshtml";
}

<div class="container-fluid">
    <div class="row articles">
        <div class="col-lg-12">
            <h1 class="generic-headline">Articles</h1>
            <p class="generic-p">Here you can find a list with links to our articles:</p>
            <div class="article-p">@Model.Value("articlesTitle")</div>
            <div>@Model.Value("articlesBodyText")</div>
        </div>

       
    </div>
</div>